#include <stdio.h>
int main() {
int i, j;
int matriz[2][3];
// Leitura dos elementos
for(i = 0; i < 2; i++) {
for(j = 0; j < 3; j++) {
printf("Digite o valor da posição [%d][%d]: ", i, j);
scanf("%d", &matriz[i][j]);
}
}
// Exibição da matriz
printf("\nMatriz digitada:\n");
for(i = 0; i < 2; i++) {
for(j = 0; j < 3; j++) {
printf("%d ", matriz[i][j]);
}
printf("\n");
}
return 0;
}
